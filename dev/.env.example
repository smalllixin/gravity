LITELLM_MASTER_KEY="sk-1234"
LITELLM_SALT_KEY="sk-1234"

# OpenTelemetry 配置
# 使用 gRPC 协议 (litellm 的 metrics 和 logs 仅支持 gRPC)
OTEL_EXPORTER_OTLP_ENDPOINT="http://otel-collector:4317"
# 传输格式 - 必须使用 grpc (metrics 和 logs 不支持 HTTP)
OTEL_EXPORTER_OTLP_PROTOCOL="grpc"

# 启用 OTEL Metrics (令牌使用、延迟、成本等指标)
LITELLM_OTEL_INTEGRATION_ENABLE_METRICS=true

# 启用 OTEL Logs/Events (提示词和响应内容的日志)
# Disabled due to OTEL protobuf bug with None values when using Langfuse protocol
# All data is available in traces anyway
LITELLM_OTEL_INTEGRATION_ENABLE_EVENTS=false